{
    "name": "hansott/lastify",
    "description": "Lastify syncs spotify playlists with your last.fm top tracks.",
    "keywords": [
        "hansott",
        "lastify"
    ],
    "homepage": "https://github.com/hansott/lastify",
    "license": "MIT",
    "authors": [
        {
            "name": "Hans Ott",
            "email": "hansott@hotmail.be",
            "homepage": "http://hansott.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "jwilsson/spotify-web-api-php": "~1.2",
        "dandelionmood/lastfm": "0.6.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "HansOtt\\Lastify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HansOtt\\Lastify\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
